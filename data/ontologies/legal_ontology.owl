        <rdfs:label xml:lang="en">crime type</rdfs:label>
        <rdfs:comment xml:lang="kk">Jinayat kategoriyası</rdfs:comment>
        <rdfs:comment xml:lang="en">Crime category</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>


