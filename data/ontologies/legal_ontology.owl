    <owl:Class rdf:about="&kk;OrtaAwırlıqtaǵıJinayat">
        <rdfs:subClassOf rdf:resource="&kk;Jinayat"/>
        <rdfs:label xml:lang="kk">Orta Awırlıqtaǵı Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Medium Severity Crime</rdfs:label>
        <rdfs:comment xml:lang="kk">Orta awırlıqtaǵı jinayat</rdfs:comment>
        <rdfs:comment xml:lang="en">Crime of medium severity</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#JeńilJinayat -->
    <owl:Class rdf:about="&kk;JeńilJinayat">
        <rdfs:subClassOf rdf:resource="&kk;Jinayat"/>
        <rdfs:label xml:lang="kk">Jeńil Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Light Crime</rdfs:label>
        <rdfs:comment xml:lang="kk">Jeńil jinayat</rdfs:comment>
        <rdfs:comment xml:lang="en">Minor crime</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#MúlikkeQarsıJinayat -->
    <owl:Class rdf:about="&kk;MúlikkeQarsıJinayat">
        <rdfs:subClassOf rdf:resource="&kk;Jinayat"/>
        <rdfs:label xml:lang="kk">Múlikke Qarsı Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Property Crime</rdfs:label>
        <rdfs:comment xml:lang="kk">Múlikke qarsı islenetuǵın jinayat</rdfs:comment>
        <rdfs:comment xml:lang="en">Crime against property</rdfs:comment>
