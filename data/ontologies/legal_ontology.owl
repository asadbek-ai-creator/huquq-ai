<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY kk "http://karakalpak.law/ontology#" >
]>

<rdf:RDF xmlns="http://karakalpak.law/ontology#"
     xml:base="http://karakalpak.law/ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:kk="http://karakalpak.law/ontology#">

    <owl:Ontology rdf:about="http://karakalpak.law/ontology">
        <rdfs:label xml:lang="kk">Qaraqalpaqstan Respublikası Huquqıy Ontologiyası</rdfs:label>
        <rdfs:label xml:lang="en">Karakalpakstan Legal System Ontology</rdfs:label>
        <rdfs:comment xml:lang="kk">Qaraqalpaqstan huquqıy sisteması ushın ontologiya</rdfs:comment>
        <rdfs:comment xml:lang="en">Ontology for Karakalpakstan legal system with native terminology</rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes - Klasslar
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- http://karakalpak.law/ontology#HuquqıyTúsinik -->
    <owl:Class rdf:about="&kk;HuquqıyTúsinik">
        <rdfs:label xml:lang="kk">Huquqıy Túsinik</rdfs:label>
        <rdfs:label xml:lang="en">Legal Concept</rdfs:label>
        <rdfs:comment xml:lang="kk">Huquqıy sistemadaǵı barlıq túsiniklerdiń negizgi klassı</rdfs:comment>
        <rdfs:comment xml:lang="en">Base class for all legal concepts in the system</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#Nızam -->
    <owl:Class rdf:about="&kk;Nızam">
        <rdfs:subClassOf rdf:resource="&kk;HuquqıyTúsinik"/>
        <rdfs:label xml:lang="kk">Nızam</rdfs:label>
        <rdfs:label xml:lang="en">Law</rdfs:label>
        <rdfs:comment xml:lang="kk">Huquqıy kodeks yamasa nızam</rdfs:comment>
        <rdfs:comment xml:lang="en">Legal code or law</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#Statiya -->
    <owl:Class rdf:about="&kk;Statiya">
        <rdfs:subClassOf rdf:resource="&kk;HuquqıyTúsinik"/>
        <rdfs:label xml:lang="kk">Statiya</rdfs:label>
        <rdfs:label xml:lang="en">Article</rdfs:label>
        <rdfs:comment xml:lang="kk">Nızamdıń bir bólimi - statiya</rdfs:comment>
        <rdfs:comment xml:lang="en">Article of a law</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#Jinayat -->
    <owl:Class rdf:about="&kk;Jinayat">
        <rdfs:subClassOf rdf:resource="&kk;HuquqıyTúsinik"/>
        <rdfs:label xml:lang="kk">Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Crime</rdfs:label>
        <rdfs:comment xml:lang="kk">Jinayat qılmıs</rdfs:comment>
        <rdfs:comment xml:lang="en">Criminal offense</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#Jaza -->
    <owl:Class rdf:about="&kk;Jaza">
        <rdfs:subClassOf rdf:resource="&kk;HuquqıyTúsinik"/>
        <rdfs:label xml:lang="kk">Jaza</rdfs:label>
        <rdfs:label xml:lang="en">Punishment</rdfs:label>
        <rdfs:comment xml:lang="kk">Jinayat ushın jaza</rdfs:comment>
        <rdfs:comment xml:lang="en">Punishment for crime</rdfs:comment>
    </owl:Class>


    <!-- http://karakalpak.law/ontology#AwırJinayat -->
    <owl:Class rdf:about="&kk;AwırJinayat">
        <rdfs:subClassOf rdf:resource="&kk;Jinayat"/>
        <rdfs:label xml:lang="kk">Awır Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Heavy Crime</rdfs:label>
        <rdfs:comment xml:lang="kk">10 jıldan artıq jaza beriletuǵın awır jinayat</rdfs:comment>
        <rdfs:comment xml:lang="en">Serious crime with punishment over 10 years</rdfs:comment>
    </owl:Class>


