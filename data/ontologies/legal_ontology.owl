    <owl:DatatypeProperty rdf:about="&kk;qabıl_etilgen_jıl">
        <rdfs:label xml:lang="kk">qabıl etilgen jıl</rdfs:label>
        <rdfs:label xml:lang="en">year adopted</rdfs:label>
        <rdfs:comment xml:lang="kk">Nızam qabıl etilgen jıl</rdfs:comment>
