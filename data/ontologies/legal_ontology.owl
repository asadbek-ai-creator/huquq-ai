        <rdfs:comment xml:lang="en">Number (same as statiya_nómiri)</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // SWRL Rules - Shegeriw Qáǵıydeleri
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!--
    Qáǵıyda 1: Eger jaza_max > 10 bolsa, ol awır jinayat
    Rule 1: If jaza_max > 10, then it is a heavy crime

    Statiya(?s) ∧ jaza_max(?s, ?jaza) ∧ swrlb:greaterThan(?jaza, 10)
        → kámil_etedi(?s, AwırJinayat)
    -->

    <!--
    Qáǵıyda 2: Eger jinayat_turi = "Múlikke qarsi" bolsa, MúlikkeQarsıJinayat klassına kiredi
    Rule 2: If jinayat_turi = "Múlikke qarsi", then it belongs to MúlikkeQarsıJinayat class

    Statiya(?s) ∧ jinayat_turi(?s, "Múlikke qarsi")
        → kámil_etedi(?s, MúlikkeQarsıJinayat)
    -->

    <!--
    Qáǵıyda 3: Eger jinayat_turi = "Adamǵa qarsi" bolsa, AdamǵaQarsıJinayat klassına kiredi
    Rule 3: If jinayat_turi = "Adamǵa qarsi", then it belongs to AdamǵaQarsıJinayat class

    Statiya(?s) ∧ jinayat_turi(?s, "Adamǵa qarsi")
        → kámil_etedi(?s, AdamǵaQarsıJinayat)
    -->

    <!--
    Qáǵıyda 4: Birday jinayat túrindegi statiyalar baylanıslı
    Rule 4: Articles with the same crime type are related

    Statiya(?s1) ∧ Statiya(?s2) ∧ jinayat_turi(?s1, ?turi) ∧ jinayat_turi(?s2, ?turi) ∧ differentFrom(?s1, ?s2)
        → baylanıslı(?s1, ?s2)
    -->

    <!--
    Qáǵıyda 5: Eger awırlıq_dárejesi = "Awır" bolsa, AwırJinayat
    Rule 5: If awırlıq_dárejesi = "Awır", then AwırJinayat

    Statiya(?s) ∧ awırlıq_dárejesi(?s, "Awır")
        → kámil_etedi(?s, AwırJinayat)
    -->

    <!--
    Qáǵıyda 6: Eger awırlıq_dárejesi = "Orta" bolsa, OrtaAwırlıqtaǵıJinayat
    Rule 6: If awırlıq_dárejesi = "Orta", then OrtaAwırlıqtaǵıJinayat

    Statiya(?s) ∧ awırlıq_dárejesi(?s, "Orta")
        → kámil_etedi(?s, OrtaAwırlıqtaǵıJinayat)
    -->

    <!--
    Qáǵıyda 7: Eger awırlıq_dárejesi = "Jeńil" bolsa, JeńilJinayat
    Rule 7: If awırlıq_dárejesi = "Jeńil", then JeńilJinayat

    Statiya(?s) ∧ awırlıq_dárejesi(?s, "Jeńil")
        → kámil_etedi(?s, JeńilJinayat)
    -->

</rdf:RDF>
