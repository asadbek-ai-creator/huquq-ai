        <rdfs:label xml:lang="kk">Adamǵa Qarsı Jinayat</rdfs:label>
        <rdfs:label xml:lang="en">Crime Against Person</rdfs:label>
        <rdfs:comment xml:lang="kk">Adam denesilikine qarsı islenetuǵın jinayat</rdfs:comment>
        <rdfs:comment xml:lang="en">Crime against person</rdfs:comment>
    </owl:Class>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties - Obekt Qásiyetleri
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- http://karakalpak.law/ontology#tiyisli -->
    <owl:ObjectProperty rdf:about="&kk;tiyisli">
        <rdfs:label xml:lang="kk">tiyisli</rdfs:label>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <rdfs:comment xml:lang="kk">Statiya qaysı nızamǵa tiyisli ekenligin kórsetedi</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates which law an article belongs to</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&kk;Nızam"/>
    </owl:ObjectProperty>


    <!-- http://karakalpak.law/ontology#baylanıslı -->
    <owl:ObjectProperty rdf:about="&kk;baylanıslı">
        <rdfs:label xml:lang="kk">baylanıslı</rdfs:label>
        <rdfs:label xml:lang="en">related to</rdfs:label>
        <rdfs:comment xml:lang="kk">Statiyalardıń arasındaǵı baylanıstı kórsetedi</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates relationship between articles</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&kk;Statiya"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
    </owl:ObjectProperty>


    <!-- http://karakalpak.law/ontology#jaza_beredi -->
    <owl:ObjectProperty rdf:about="&kk;jaza_beredi">
        <rdfs:label xml:lang="kk">jaza beredi</rdfs:label>
        <rdfs:label xml:lang="en">gives punishment</rdfs:label>
        <rdfs:comment xml:lang="kk">Statiya qanday jaza berip atırǵanın kórsetedi</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates what punishment an article prescribes</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&kk;Jaza"/>
    </owl:ObjectProperty>


    <!-- http://karakalpak.law/ontology#kámil_etedi -->
    <owl:ObjectProperty rdf:about="&kk;kámil_etedi">
        <rdfs:label xml:lang="kk">kámil etedi</rdfs:label>
        <rdfs:label xml:lang="en">constitutes</rdfs:label>
        <rdfs:comment xml:lang="kk">Statiya qanday jinayattı kámil etedi</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates what crime an article constitutes</rdfs:comment>
        <rdfs:domain rdf:resource="&kk;Statiya"/>
        <rdfs:range rdf:resource="&kk;Jinayat"/>
    </owl:ObjectProperty>


