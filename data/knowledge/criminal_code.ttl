# ============================================================================
# Statiya 169 - Qasdın adam óltiriw
# Article 169 - Intentional Murder
# ============================================================================

kk:Statiya_169
    a kk:Statiya ;
    kk:nómiri 169 ;
    kk:statiya_nómiri 169 ;
    kk:sárelaw "Qasdın adam óltiriw"@kk ;
    kk:tekstı "Qasdın adam óltiriw - on eki jıldan on tórt jılǵa shekem erkinlikten ayırıw menen jazalanadi."@kk ;
    kk:jaza_min 12 ;
    kk:jaza_max 14 ;
    kk:jaza_turi "Erkinlikten ayırıw" ;
    kk:jinayat_turi "Adamǵa qarsi" ;
    kk:awırlıq_dárejesi "Awır" ;
    kk:nızam_id "JinayatKodeksi_2023" ;
    kk:tiyisli kk:JinayatKodeksi_2023 ;
    rdfs:label "Statiya 169 - Qasdın adam óltiriw"@kk ;
    rdfs:label "Article 169 - Intentional Murder"@en ;
    rdfs:comment "Adamdı qasdın óltirgenlik ushın jaza"@kk .


# ============================================================================
# Statiya 175 - Ūrlıq (O'g'irliq)
# Article 175 - Theft
# ============================================================================

kk:Statiya_175
    a kk:Statiya ;
    kk:nómiri 175 ;
    kk:statiya_nómiri 175 ;
    kk:sárelaw "Ūrlıq"@kk ;
    kk:tekstı "Baska adamǵa tiyisli múlikti jasırın tartıp alıw (ūrlıq) - úsh jılǵa shekem erkinlikten ayırıw yamasa aqsha jazası menen jazalanadi."@kk ;
    kk:jaza_min 0 ;
    kk:jaza_max 3 ;
    kk:jaza_turi "Erkinlikten ayırıw yamasa aqsha jazası" ;
    kk:jinayat_turi "Múlikke qarsi" ;
    kk:awırlıq_dárejesi "Orta" ;
    kk:nızam_id "JinayatKodeksi_2023" ;
    kk:tiyisli kk:JinayatKodeksi_2023 ;
    rdfs:label "Statiya 175 - Ūrlıq"@kk ;
    rdfs:label "Article 175 - Theft"@en ;
    rdfs:comment "Basqanıń múlikin jasırın alıw"@kk .


# ============================================================================
# Statiya 177 - Talaw (Talon-taroj)
# Article 177 - Robbery
# ============================================================================

kk:Statiya_177
    a kk:Statiya ;
    kk:nómiri 177 ;
    kk:statiya_nómiri 177 ;
    kk:sárelaw "Talaw"@kk ;
    kk:tekstı "Kúsh qollaw yamasa onı qorqıtıw menen baska adamǵa tiyisli múlikti tartıp alıw - bes jıldan on eki jılǵa shekem erkinlikten ayırıw menen jazalanadi."@kk ;
    kk:jaza_min 5 ;
    kk:jaza_max 12 ;
    kk:jaza_turi "Erkinlikten ayırıw" ;
    kk:jinayat_turi "Múlikke qarsi" ;
    kk:awırlıq_dárejesi "Awır" ;
    kk:nızam_id "JinayatKodeksi_2023" ;
