    rdfs:label "Statiya 177 - Talaw"@kk ;
    rdfs:label "Article 177 - Robbery"@en ;
    rdfs:comment "Kúsh qollaw menen múlik tartıp alıw"@kk .


# ============================================================================
# Statiya 168 - Aldaw (Firibgarlik)
# Article 168 - Fraud
# ============================================================================

kk:Statiya_168
    a kk:Statiya ;
    kk:nómiri 168 ;
    kk:statiya_nómiri 168 ;
    kk:sárelaw "Aldaw"@kk ;
    kk:tekstı "Basqa adamdı aldaw yamasa senim-ishenim paydalanıp múlik yamasa múlikke bolǵan huquqtı iye etiw - úsh jılǵa shekem erkinlikten ayırıw yamasa aqsha jazası menen jazalanadi."@kk ;
    kk:jaza_min 0 ;
    kk:jaza_max 3 ;
    kk:jaza_turi "Erkinlikten ayırıw yamasa aqsha jazası" ;
    kk:jinayat_turi "Múlikke qarsi" ;
    kk:awırlıq_dárejesi "Orta" ;
    kk:nızam_id "JinayatKodeksi_2023" ;
    kk:tiyisli kk:JinayatKodeksi_2023 ;
    rdfs:label "Statiya 168 - Aldaw"@kk ;
    rdfs:label "Article 168 - Fraud"@en ;
    rdfs:comment "Adamdı aldaw arqalı múlik alıw"@kk .


